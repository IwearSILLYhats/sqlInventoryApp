<fieldset>
    <legend><%= header %></legend>
    <% if (typeof data !== "undefined") { %>
        <% data.forEach(item => { %>
            <div>
                <label for="<%= item.name %>"><%= item.name %></label>
                <input type="checkbox" name="<%= header.toLowerCase() %>" id="<%= item.name %>" value="<%= item.id %>" <%= item.hasOwnProperty("checked") && item.checked ? "checked" : "" %>>
            </div>
        <% }) %>
    <% } else { %>
        <p>No <%= header.toLowerCase() %> found</p>
    <% } %>
</fieldset>